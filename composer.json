{
    "name": "simcript/cuter",
    "description": "A link shorthand",
    "keywords": ["laravel", "cuter", "link", "shorthand"],
    "license": "MIT",
    "authors": [
        {
            "name": "AliA MehR",
            "email": "alia_mehr@yahoo.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "5.5.x|5.6.x|5.7.x",
        "symfony/http-foundation": "^3.1|^4",
        "symfony/http-kernel": "^3.1|^4"
    },
    "autoload": {
        "psr-4": {
            "Simcript\\Cuter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "laravel": {
           "dont-discover": [
                "Simcript\\Cuter\\" 
            ],
            "providers": [
                "Simcript\\Cuter\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^5.5"
    },
    "scripts": {
    },
     "minimum-stability": "dev",
    "prefer-stable": true
}
