{
    "name": "simcript/cuter",
    "description": "A link shorthand",
    "type": "metapackage",
    "keywords": ["laravel", "cuter", "link", "shorthand"],
    "license": "MIT",
    "authors": [
        {
            "name": "AliA MehR",
            "email": "alia_mehr@yahoo.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "5.5.x|5.6.x|5.7.x",
        "symfony/http-foundation": "^3.1|^4",
        "symfony/http-kernel": "^3.1|^4"
    },
    "autoload": {
        "psr-4": {
            "Simcript\\Cuter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simcript\\Cuter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        },
        "laravel": {
            "providers": [
                "Simcript\\Cuter\\CuterServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^5.5",
        "phpunit/phpunit": "^4.8|^5.2|^7.0",
        "orchestra/testbench": "3.3.x|3.4.x|3.5.x|3.6.x|3.7.x",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
     "minimum-stability": "stable",
    "prefer-stable": true
}
